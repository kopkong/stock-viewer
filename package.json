{
  "name": "stock",
  "version": "0.1.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "concurrently --kill-others \"ng serve --proxy-config proxy.conf.json\" \"npm run _server:run\"",
    "start-db": "mongod",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "build": "ng build --prod --output-path=dist/client && npm run _server:build",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "_server:build": "tsc -p ./server",
    "deploy": "pm2 deploy ecosystem.config.js production --force"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "@types/body-parser": "0.0.32",
    "@types/compression": "0.0.33",
    "@types/cors": "0.0.33",
    "@types/express": "^4.0.35",
    "@types/jasmine": "^2.5.47",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/node": "^6.0.68",
    "@types/node-uuid": "0.0.28",
    "co": "^4.6.0",
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "iconv-lite": "^0.4.15",
    "karma-jasmine": "^1.1.0",
    "karma-remap-istanbul": "^0.2.2",
    "mongodb": "^2.2.25",
    "normalize.css": "^5.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-remember-me": "0.0.1",
    "rxjs": "^5.1.0",
    "ts-node": "~2.0.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/compiler-cli": "^4.0.1",
    "codelyzer": "~1.0.0-beta.3",
    "concurrently": "^3.1.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "nodemon": "^1.11.0",
    "protractor": "4.0.9",
    "tslint": "~4.5.1",
    "typescript": "~2.1.0",
    "webdriver-manager": "10.2.5"
  }
}
